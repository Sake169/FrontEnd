{
  "name": "securities-filing-frontend",
  "version": "1.0.0",
  "description": "证券公司员工配偶信息报备系统前端",
  "main": "index.js",
  "scripts": {
    "dev": "cd frontend-app && npm run dev",
    "build": "cd frontend-app && npm run build",
    "preview": "cd frontend-app && npm run preview",
    "backend": "cd backend-service && python start.py",
    "start": "concurrently \"npm run backend\" \"npm run dev\"",
    "lint": "cd frontend-app && eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "cd frontend-app && eslint src --ext .js,.jsx,.ts,.tsx --fix"
  },
  "dependencies": {
    "@ant-design/icons": "^5.2.6",
    "@ant-design/pro-components": "^2.6.48",
    "@ant-design/pro-layout": "^7.17.16",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.2.0",
    "@mui/material": "^7.2.0",
    "@types/react-router-dom": "^5.3.3",
    "ag-grid-community": "^34.1.0",
    "ag-grid-react": "^34.1.0",
    "antd": "^5.12.8",
    "axios": "^1.11.0",
    "file-saver": "^2.0.5",
    "luckysheet": "^2.1.13",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^7.7.1",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/file-saver": "^2.0.7",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^8.2.2",
    "eslint": "^8.55.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "typescript": "^5.2.2",
    "vite": "^5.0.8"
  },
  "keywords": [
    "react",
    "antd",
    "securities",
    "filing",
    "excel"
  ],
  "author": "Securities Filing System",
  "license": "MIT"
}
